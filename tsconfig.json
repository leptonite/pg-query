{
   "compilerOptions": {
      "target": "ES2021",
      "lib": [
         "ES2021"
      ],
      "module": "commonjs",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "outDir": "./build",
      "newLine": "lf",
      "stripInternal": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
   }
}
